generator client {
  provider = "prisma-client-js"
  output = "C:\\All-Program\\DynAPI\\server\\generated\\prisma_builds\\1762242304674"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  password  String
  role      Role     @default(ADMIN)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

enum Role {
  ADMIN
  MANAGER
  VIEWER
}

model Apple {
  id        Int      @id @default(autoincrement())
  Kind String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}


model Ferrari {
  id        Int      @id @default(autoincrement())
  Nmae String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}



model Ferra {
  id        Int      @id @default(autoincrement())
  Nmae String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

